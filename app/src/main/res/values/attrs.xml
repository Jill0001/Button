<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JellyRefreshLayout">
        <attr name="jellyColor" format="color"/>
        <attr name="headerHeight" format="dimension"/>
        <attr name="pullHeight" format="dimension"/>
        <attr name="triggerHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundBackgroundColor" format="color"/>
        <attr name="roundForegroundColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="percent" format="float"></attr>
        <attr name="zeroPointRoundSize" format="dimension"></attr>
        <attr name="zeroPointRoundColor" format="color"></attr>
    </declare-styleable>

</resources>